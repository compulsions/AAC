
 var cores=[
        "rgb(0,70,127)", //letras
        "rgb(238,50,36)", //direito
        "rgb(255,196,37)", //medicina
        "rgb(0,150,215)", // c e tecnologia
        "rgb(83,36,127)", // farmacia
        "rgb(249,157,49)", //psicologia
        "rgb(85,25,0)"]; //desporto

     window.onload = function() {
        randomColor();
        
        }
     function randomColor(){
        var r = parseInt(Math.random()*cores.length);
        var i = cores[r];
     }
